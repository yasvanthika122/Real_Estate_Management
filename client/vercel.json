{
    "version": 2,
    "builds": [
      {
        "src": "index.js", 
        "use": "@vercel/node"
      }
     
    ],
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/api/(.*)",
        "dest": "index.js" 
      },
      {
        "src": "/(.*)",
        "dest": "client/index.html" 
      }
    ]
  }
  